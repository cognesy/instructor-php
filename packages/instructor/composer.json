{
  "name": "cognesy/instructor-struct",
  "description": "Structured data extraction in PHP, powered by LLMs - core library",
  "keywords": [
    "llm",
    "language models",
    "inference",
    "ai",
    "genai",
    "openai",
    "anthropic",
    "cohere",
    "ollama",
    "structured output",
    "semantic processing",
    "automation",
    "data processing",
    "data extraction"
  ],
  "type": "library",
  "license": "MIT",
  "bin": [
    "bin/instructor"
  ],
  "homepage": "https://instructorphp.com",
  "support": {
    "issues": "https://github.com/cognesy/instructor-php/issues",
    "source": "https://github.com/cognesy/instructor-php"
  },
  "authors": [
    {
      "name": "Dariusz Debowczyk",
      "email": "ddebowczyk@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Cognesy\\Instructor\\": "src/"
    },
    "files": []
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "php-http/discovery": true
    },
    "sort-packages": true
  },
  "require": {
    "php": "^8.2",
    "cognesy/instructor-utils": "^0.12",
    "cognesy/instructor-setup": "^0.12",
    "cognesy/instructor-polyglot": "^0.12",
    "cognesy/instructor-addons": "^0.12",
    "phpdocumentor/reflection-docblock": "^5.4",
    "phpstan/phpdoc-parser": "^1.29",
    "symfony/property-access": "^6.4 || ^7.0",
    "symfony/property-info": "^6.4 || ^7.0",
    "symfony/serializer": "^6.4 || ^7.0",
    "symfony/type-info": "^7.1",
    "symfony/validator": "^6.4 || ^7.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest"
  }
}
