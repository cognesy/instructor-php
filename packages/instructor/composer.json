{
  "name": "cognesy/instructor-struct",
  "license": "MIT",
  "description": "Structured data extraction in PHP, powered by LLMs - core library",
  "keywords": [
    "llm",
    "language models",
    "inference",
    "ai",
    "genai",
    "openai",
    "anthropic",
    "cohere",
    "ollama",
    "structured output",
    "semantic processing",
    "automation",
    "data processing",
    "data extraction"
  ],
  "type": "library",
  "homepage": "https://instructorphp.com",
  "support": {
    "issues": "https://github.com/cognesy/instructor-php/issues",
    "source": "https://github.com/cognesy/instructor-php"
  },
  "authors": [
    {
      "name": "Dariusz Debowczyk",
      "email": "ddebowczyk@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Cognesy\\Instructor\\": "src/"
    },
    "files": []
  },
  "autoload-dev": {
    "psr-4": {
      "Cognesy\\Instructor\\Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    },
    "sort-packages": true
  },
  "require": {
    "php": "^8.2",
    "cognesy/instructor-config": "^1.0",
    "cognesy/instructor-events": "^1.0",
    "cognesy/instructor-polyglot": "^1.0",
    "cognesy/instructor-setup": "^1.0",
    "cognesy/instructor-templates": "^1.0",
    "cognesy/instructor-utils": "^1.0",
    "phpdocumentor/reflection-docblock": "^5.4",
    "phpstan/phpdoc-parser": "^1.29",
    "symfony/property-access": "^6.4 || ^7.0",
    "symfony/property-info": "^6.4 || ^7.0",
    "symfony/serializer": "^6.4 || ^7.0",
    "symfony/validator": "^6.4 || ^7.0"
  },
  "require-dev": {
    "cognesy/instructor-addons": "^1.0",
    "cognesy/instructor-schema": "^1.0",
    "roave/security-advisories": "dev-latest",
    "mockery/mockery": "^1.6",
    "icanhazstring/composer-unused": "^0.9.0",
    "jetbrains/phpstorm-attributes": "^1.2",
    "maglnet/composer-require-checker": "^4.16",
    "pestphp/pest": "^2.34",
    "phpstan/phpstan": "^1.11",
    "vimeo/psalm": "^6.0"
  },
  "suggest": {
    "cognesy/instructor-addons": "Extra capabilities for Instructor and Polyglot packages",
    "cognesy/instructor-schema": "For Symfony 7+ schema validation and management",
    "cognesy/instructor-schema-v6": "For Symfony 6 schema validation and management"
  },
  "scripts": {
    "tests": "@php vendor/bin/pest",
    "phpstan": "@php vendor/bin/phpstan -c phpstan.neon",
    "psalm": "@php vendor/bin/psalm"
  },
  "minimum-stability": "RC",
  "prefer-stable": true
}
