{
  "name": "cognesy/instructor-hub",
  "version": "1.0.0-RC7",
  "description": "CLI tool to work with Instructor PHP code examples and docs",
  "license": "MIT",
  "homepage": "https://instructorphp.com",
  "support": {
    "issues": "https://github.com/cognesy/instructor-php/issues",
    "source": "https://github.com/cognesy/instructor-php"
  },
  "authors": [
    {
      "name": "Dariusz Debowczyk",
      "email": "ddebowczyk@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Cognesy\\InstructorHub\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Cognesy\\InstructorHub\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.2",
    "cognesy/instructor-struct": "^1.0",
    "cognesy/instructor-auxiliary": "^1.0",
    "toolkit/cli-utils": "^2.0",
    "cebe/markdown": "^1.2"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "icanhazstring/composer-unused": "^0.9.0",
    "jetbrains/phpstorm-attributes": "^1.2",
    "maglnet/composer-require-checker": "^4.16",
    "pestphp/pest": "^2.34",
    "phpstan/phpstan": "^1.11",
    "vimeo/psalm": "^6.0"
  },
  "scripts": {
    "tests": "@php vendor/bin/pest",
    "phpstan": "@php vendor/bin/phpstan -c phpstan.neon",
    "psalm": "@php vendor/bin/psalm",
    "hub": "bin/instructor-hub"
  },
  "bin": [
    "bin/instructor-hub"
  ],
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    },
    "sort-packages": true
  },
  "minimum-stability": "RC",
  "prefer-stable": true
}
