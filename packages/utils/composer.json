{
  "name": "cognesy/instructor-utils",
  "version": "0.17.8",
  "description": "Collection of utility classes used by Instructor library",
  "license": "MIT",
  "homepage": "https://instructorphp.com",
  "support": {
    "issues": "https://github.com/cognesy/instructor-php/issues",
    "source": "https://github.com/cognesy/instructor-php"
  },
  "authors": [
    {
      "name": "Dariusz Debowczyk",
      "email": "ddebowczyk@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Cognesy\\Utils\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Cognesy\\Utils\\Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    },
    "sort-packages": true
  },
  "require": {
    "php": "^8.2",
    "ext-ctype": "*",
    "ext-fileinfo": "*",
    "ext-libxml": "*",
    "ext-simplexml": "*",
    "ext-xmlreader": "*",
    "adbario/php-dot-notation": "^3.3",
    "aimeos/map": "^3.8",
    "psr/event-dispatcher": "^1.0",
    "psr/log": "^3.0",
    "vlucas/phpdotenv": "^5.6",
    "webuni/front-matter": "^2.0",
    "twig/twig": "^3.0",
    "gioni06/gpt3-tokenizer": "^1.2"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "icanhazstring/composer-unused": "^0.9.0",
    "jetbrains/phpstorm-attributes": "^1.2",
    "maglnet/composer-require-checker": "^4.16",
    "pestphp/pest": "^2.34",
    "phpstan/phpstan": "^1.11",
    "vimeo/psalm": "^6.0",
    "eftec/bladeone": "^4.16"
  },
  "scripts": {
    "tests": "@php vendor/bin/pest",
    "phpstan": "@php vendor/bin/phpstan -c phpstan.neon",
    "psalm": "@php vendor/bin/psalm"
  }
}
