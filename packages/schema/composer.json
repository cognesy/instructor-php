{
  "name": "cognesy/instructor-schema",
  "description": "Object to schema conversion for Instructor library, should be used with Instructor",
  "license": "MIT",
  "homepage": "https://instructorphp.com",
  "support": {
    "issues": "https://github.com/cognesy/instructor-php/issues",
    "source": "https://github.com/cognesy/instructor-php"
  },
  "authors": [
    {
      "name": "Dariusz Debowczyk",
      "email": "ddebowczyk@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Cognesy\\Schema\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Cognesy\\Schema\\Tests\\": "tests/"
    },
    "files": []
  },
  "require": {
    "php": "^8.2",
    "cognesy/instructor-utils": "^1.5",
    "cognesy/instructor-pipeline": "^1.5",
    "symfony/property-info": "^6.4|^7.3"
  },
  "require-dev": {
    "icanhazstring/composer-unused": "^0.9.0",
    "jetbrains/phpstorm-attributes": "^1.2",
    "maglnet/composer-require-checker": "^4.16",
    "pestphp/pest": "^2.34",
    "phpstan/phpstan": "^1.11",
    "roave/security-advisories": "dev-latest",
    "symfony/var-dumper": "^7.3",
    "vimeo/psalm": "^6.0"
  },
  "scripts": {
    "tests": "@php vendor/bin/pest",
    "phpstan": "@php vendor/bin/phpstan -c phpstan.neon",
    "psalm": "@php vendor/bin/psalm"
  },
  "suggest": {
    "symfony/type-info": "TypeInfo replaces deprecated PropertyInfo types in Symfony 7.3+, but is not supported in Symfony 6.4."
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    },
    "sort-packages": true
  },
  "minimum-stability": "RC",
  "prefer-stable": true
}
